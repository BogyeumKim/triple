<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.gaerine.triple.mapper.TokenMapper">
    <insert id="insertToken"  useGeneratedKeys="true" keyProperty="id">
        insert into token (member_id,access_token,refresh_token) values (#{member_id},#{access_token},#{refresh_token});
    </insert>
</mapper>